{
	"manifest_version": 2,

	"name": "Fingerprint Anonymizer",
	"description": "Removes identifying information to reduce or prevent browser fingerprinting.",
	"version": "1.0",
	"permissions": [
		"<all_urls>"
	],
	"browser_action": {
	"default_icon": "icon.png",
	"default_popup": "popup.html"
	},
	"content_scripts": [
		{
			"matches": [
				"<all_urls>"
			],
			"js": [
				"injected.js"
			],
			"run_at": "document_start",
			"all_frames": true
		}
	]
}